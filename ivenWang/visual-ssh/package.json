    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@types/jest": "^27.4.1",
    "@types/jquery": "^3.5.6",
    "electron": "^14.0.0",
    "electron-builder": "^22.13.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@types/ssh2": "^0.5.48",
