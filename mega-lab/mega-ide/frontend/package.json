{
  "private": true,
  "scripts": {
    "start": "cross-env ANALYZE=1 umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-card": "^1.18.26",
    "@ant-design/pro-descriptions": "^1.10.36",
    "@ant-design/pro-form": "^1.52.14",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-list": "^1.21.21",
    "@ant-design/pro-skeleton": "^1.0.4",
    "@ant-design/pro-table": "^2.62.8",
    "@antv/g2": "^4.1.42",
    "@react-keycloak/web": "3.4.0",
    "keycloak-js": "16.1.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-polling": "^1.0.9",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
