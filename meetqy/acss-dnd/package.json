{
  "name": "acss-dnd",
  "version": "0.0.8",
  "description": "acss-dnd是一款基于原子类 css 特性，实现拖拽布局的一个工具。",
  "keywords": [
    "tailwindcss",
    "acss-dnd",
    "css",
    "daisyui",
    "dnd",
    "drag and drop",
    "atomic css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "cz": "git cz",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "bugs": {
    "url": "https://github.com/cu-yang/acss-dnd/issues"
  },
  "homepage": "https://github.com/cu-yang/acss-dnd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cu-yang/acss-dnd.git"
  },
  "author": "cuyang",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@vueuse/core": "^7.7.0",
    "daisyui": "^2.6.4",
    "highlight.js": "^11.5.0",
    "pinia": "^2.0.11",
    "uuid": "^8.3.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rushstack/eslint-patch": "^1.1.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^16.11.25",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.2",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "lint-staged": ">=10",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vue-tsc": "^0.31.4"
  }
}
