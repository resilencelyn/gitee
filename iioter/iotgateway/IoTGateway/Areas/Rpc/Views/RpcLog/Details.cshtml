@model IoTGateway.ViewModel.Rpc.RpcLogVMs.RpcLogVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.RpcSide" />
<wt:display field="Entity.StartTime" />
<wt:display field="Entity.Device.DeviceName" />
<wt:display field="Entity.Method" />
<wt:display field="Entity.Params" />
<wt:display field="Entity.EndTime" />
<wt:display field="Entity.IsSuccess" />
<wt:display field="Entity.Description" />
</wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:closebutton />
    </wt:row>
</wt:form>
