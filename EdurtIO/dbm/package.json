    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ts-loader": "^9.2.3",
    "typescript": "4.2.4",
    "wait-on": "^5.3.0",
    "webpack": "5.39.1",
    "webpack-cli": "4.7.2"
  },
